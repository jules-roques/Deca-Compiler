class Item {
    private float price;
    private int quantity;

    public Item(float price, int quantity) {
        this.price = price;
        this.quantity = quantity;
    }

    public float calculateTotal() {
        return price * quantity;
    }
}


{
    Item item1 = new Item(10.99, 3);
    Item item2 = new Item(24.99, 2);
    Item item3 = new Item(19.99, 1);

    println(item1.calculateTotal());
}
