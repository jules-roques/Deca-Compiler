class ClasseAvecMethodes {
    // Méthode avec signature spécifique
    int multiplier(int a, int b) {
        return a * b;
    }
}

class ClasseTest {
    void executionTest() {
        ClasseAvecMethodes objet = new ClasseAvecMethodes();

        // Appel valide de la méthode avec la signature appropriée
        int resultat = objet.multiplier(3, 4);
        println("Résultat de la multiplication: " + resultat);
    }
}

// Programme principal
{
    ClasseTest testProgramme = new ClasseTest();
    testProgramme.executionTest();
}
